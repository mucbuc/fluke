namespace om636
{
    /////////////////////////////////////////////////////////////////////////////////////////////
    // batch_layer<T>
    /////////////////////////////////////////////////////////////////////////////////////////////
    template<class T, class U>
    void batch_layer<T, U>::process( serving_type & s )
    {
        s = serving_type( & m_data );
    }
    
    /////////////////////////////////////////////////////////////////////////////////////////////
    template<class T, class U>
    void batch_layer<T, U>::insert( value_type i )
    {
        m_data.append( i );
    }
    
    /////////////////////////////////////////////////////////////////////////////////////////////
    template<class T, class U>
    auto batch_layer<T, U>::data() -> container_type &
    {
        return m_data;
    }
    
    /////////////////////////////////////////////////////////////////////////////////////////////
    template<class T, class U>
    auto batch_layer<T, U>::data() const -> const container_type &
    {
        return m_data;
    }
}